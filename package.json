{
  "name": "killbill",
  "version": "0.1.0",
  "description": "Kill Bill client library",
  "homepage": "https://github.com/killbill/killbill-client-js",
  "author": "Kill Bill core team <killbilling-users@googlegroups.com> (http://kill-bill.org/)",
  "repository": {
    "type": "git",
    "url": "git://github.com/killbill/killbill-client-js.git"
  },
  "bugs:": "https://github.com/killbill/killbill-client-js/issues",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf src dist",
    "codegen": "npm run clean && openapi-generator generate --generator-name typescript-axios --input-spec kbswagger.yaml --output src --additional-properties supportsES6=true",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^0.20.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.18-4.3.1",
    "@types/node": "^14.11.2",
    "typescript": "^4.0.3"
  }
}
